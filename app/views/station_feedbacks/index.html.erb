<p id="notice"><%= notice %></p>

<h1>Station Feedbacks</h1>

<table>
  <thead>
    <tr>
      <th>Azione</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @station_feedbacks.each do |station_feedback| %>
      <tr>
        <td><%= station_feedback.azione %></td>
        <td><%= link_to 'Show', station_feedback %></td>
        <td><%= link_to 'Edit', edit_station_feedback_path(station_feedback) %></td>
        <td><%= link_to 'Destroy', station_feedback, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Station Feedback', new_station_feedback_path %>
