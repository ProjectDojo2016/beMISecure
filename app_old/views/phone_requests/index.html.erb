<p id="notice"><%= notice %></p>

<h1>Phone Requests</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @phone_requests.each do |phone_request| %>
      <tr>
        <td><%= link_to 'Show', phone_request %></td>
        <td><%= link_to 'Edit', edit_phone_request_path(phone_request) %></td>
        <td><%= link_to 'Destroy', phone_request, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Phone Request', new_phone_request_path %>
